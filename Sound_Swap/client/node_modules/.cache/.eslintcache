s/Playlist\";\nimport { MdRouter } from \"react-icons/md\";\n\nfunction App() {\n  const [user, setUser] = useState(null);\n\n  useEffect(() => {\n    // auto-login\n    fetch(\"/me\").then((r) => {\n      if (r.ok) {\n        r.json().then((user) => setUser(user));\n      }\n    });\n  }, []);\n\n  if (!user) return <Login onLogin={setUser} />;\n  //test\n  return (\n    <>\n      <NavBar user={user} setUser={setUser} />\n      <Switch>\n        <Route path=\"/search\" component={() => <Search />} />\n        <Route path=\"/playlist\" component={() => <Playlist />} />\n      </Switch>\n    </>\n  );\n}\n\nexport default App;\n","/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/components/NavBar.js",["77"],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/pages/Login.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/pages/Search.js",["78","79"],"import React, { useEffect, useState } from \"react\";\nimport SongCard from \"./SongCard\";\n\nconst SearchBar = () => {\n  const [search, setSearch] = useState(\"\");\n  const [results, setResults] = useState([]);\n  const [update, setUpdate] = useState(false);\n\n  const deezer = `https://api.deezer.com/search?q=${search}&redirect_uri=http%253A%252F%252Fguardian.mashape.com%252Fcallback&index=25\n  `;\n\n  function handleSearch(e) {\n    e.preventDefault();\n    setUpdate(!update);\n  }\n\n  useEffect(() => {\n    if (search == \"\") {\n    } else {\n      fetch(deezer)\n        .then((res) => res.json())\n        .then((songData) => setResults(songData.data));\n    }\n  }, [update]);\n  // console.log(results);\n  const handleChange = (e) => {\n    setSearch(e.target.value);\n  };\n\n  return (\n    <form id=\"search\" onSubmit={handleSearch}>\n      <input\n        type=\"text\"\n        id=\"search\"\n        placeholder=\"Search\"\n        name=\"s\"\n        onChange={handleChange}\n      />\n      <button className=\"button\" type=\"submit\">\n        Search\n      </button>\n      <SongCard results={results} />\n    </form>\n  );\n};\n\nexport default SearchBar;\n","/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/pages/Playlist.js",["80","81"],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/components/SignUpForm.js",["82"],"import React, { useState } from \"react\";\nimport { Button, Error, Input, FormField, Label, Textarea } from \"../styles\";\n\nfunction SignUpForm({ onLogin }) {\n  const [username, setUsername] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [errors, setErrors] = useState([]);\n  const [isLoading, setIsLoading] = useState(false);\n\n  function handleSubmit(e) {\n    e.preventDefault();\n    setErrors([]);\n    setIsLoading(true);\n    fetch(\"/signup\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify({\n        username,\n        password,\n      }),\n    }).then((r) => {\n      setIsLoading(false);\n      if (r.ok) {\n        r.json().then((user) => onLogin(user));\n      } else {\n        r.json().then((err) => setErrors(err.errors));\n      }\n    });\n  }\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <FormField>\n        <Label htmlFor=\"username\">Username</Label>\n        <Input\n          type=\"text\"\n          id=\"username\"\n          autoComplete=\"off\"\n          value={username}\n          onChange={(e) => setUsername(e.target.value)}\n        />\n      </FormField>\n      <FormField>\n        <Label htmlFor=\"password\">Password</Label>\n        <Input\n          type=\"password\"\n          id=\"password\"\n          value={password}\n          onChange={(e) => setPassword(e.target.value)}\n          autoComplete=\"current-password\"\n        />\n      </FormField>\n\n      <FormField>\n        <Button color=\"secondary\" type=\"submit\">{isLoading ? \"Loading...\" : \"Sign Up\"}</Button>\n      </FormField>\n      <FormField>\n        {errors.map((err) => (\n          <Error key={err}>{err}</Error>\n        ))}\n      </FormField>\n    </form>\n  );\n}\n\nexport default SignUpForm;\n","/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/components/LoginForm.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/pages/SongCard.js",["83","84"],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/styles/index.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/styles/Input.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/styles/Button.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/styles/FormField.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/styles/Textarea.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/styles/Error.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/styles/Box.js",[],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/styles/Label.js",[],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":7,"column":10,"nodeType":"91","messageId":"92","endLine":7,"endColumn":18},{"ruleId":"89","severity":1,"message":"93","line":4,"column":10,"nodeType":"91","messageId":"92","endLine":4,"endColumn":17},{"ruleId":"94","severity":1,"message":"95","line":18,"column":16,"nodeType":"96","messageId":"97","endLine":18,"endColumn":18},{"ruleId":"98","severity":1,"message":"99","line":24,"column":6,"nodeType":"100","endLine":24,"endColumn":14,"suggestions":"101"},{"r