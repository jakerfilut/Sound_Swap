n((songData) => setResults(songData.data));\n    }\n  }, [update]);\n  // console.log(results);\n  const handleChange = (e) => {\n    setSearch(e.target.value);\n  };\n\n  return (\n    <form id=\"search\" onSubmit={handleSearch}>\n      <input\n        type=\"text\"\n        id=\"search\"\n        placeholder=\"Search\"\n        name=\"s\"\n        onChange={handleChange}\n      />\n      <button className=\"button\" type=\"submit\">\n        Search\n      </button>\n      <SongCard results={results} />\n    </form>\n  );\n};\n\nexport default SearchBar;\n","/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/pages/Playlist.js",["81"],"/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/components/SignUpForm.js",["82"],"import React, { useState } from \"react\";\nimport { Button, Error, Input, FormField, Label, Textarea } from \"../styles\";\n\nfunction SignUpForm({ onLogin }) {\n  const [username, setUsername] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [errors, setErrors] = useState([]);\n  const [isLoading, setIsLoading] = useState(false);\n\n  function handleSubmit(e) {\n    e.preventDefault();\n    setErrors([]);\n    setIsLoading(true);\n    fetch(\"/signup\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify({\n        username,\n        password,\n      }),\n    }).then((r) => {\n      setIsLoading(false);\n      if (r.ok) {\n        r.json().then((user) => onLogin(user));\n      } else {\n        r.json().then((err) => setErrors(err.errors));\n      }\n    });\n  }\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <FormField>\n        <Label htmlFor=\"username\">Username</Label>\n        <Input\n          type=\"text\"\n          id=\"username\"\n          autoComplete=\"off\"\n          value={username}\n          onChange={(e) => setUsername(e.target.value)}\