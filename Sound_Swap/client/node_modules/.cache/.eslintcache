ort Playlist from \"../pages/Playlist\";\nimport { MdRouter } from \"react-icons/md\";\n\nfunction App() {\n  const [user, setUser] = useState(null);\n\n  useEffect(() => {\n    // auto-login\n    fetch(\"/me\").then((r) => {\n      if (r.ok) {\n        r.json().then((user) => setUser(user));\n      }\n    });\n  }, []);\n\n  if (!user) return <Login onLogin={setUser} />;\n  //test\n  return (\n    <>\n      <NavBar user={user} setUser={setUser} />\n      <Switch>\n        <Route path=\"/search\" component={() => <Search />} />\n        <Route path=\"/playlist\" component={() => <Playlist />} />\n      </Switch>\n    </>\n  );\n}\n\nexport default App;\n","/Users/francescah12/development/code/mod4/new-sound-swap/Sound_Swap/Sound_Swap/client/src/components/NavBar.js",["78"],"import { React, useState } from \"react\";\nimport \"./NavBar.css\";\nimport { IconContext } from \"react-icons/lib\";\nimport { FaTools, FaBars, FaTimes } from \"react-icons/fa\";\nimport { MdOutlineLibraryMusic } from \"react-icons/md\";\nimport { SiAddthis } from \"react-icons/si\";\nimport { BsPersonSquare } from \"react-icons/bs\";\nimport { Link } from \"react-router-dom\";\n\nfunction NavBar({ user, setUser }) {\n  const [click, setClick] = useState(false);\n  const handleClick = () => setClick(!click);\n  const closeMobileMenu = () => setClick(false);\n  //test\n  function handleLogoutClick() {\n    fetch(\"/logout\", { method: \"DELETE\" }).then((r) => {\n      if (r.ok) {\n        setUser(null);\n      }\n    });\n  }\n\n  return (\n    <IconContext.Provider value={{ color: \"#EE6C4D\", size: 40 }}>\n      <div className=\"navbar\">\n        <div className=\"navbar-container container\">\n          <div className=\"navbar-logo\" onClick={closeMobileMenu}>\n            <MdOutlineLibraryMusic />\n            SoundSwap\n          </div>\n          <div className=\"menu-icon\" onClick={handleClick}>\n            {click ? <FaTimes /> : <FaBars />}\n          </div>\n          <ul className={